<!DOCTYPE html>
<html>
<head>
    <title>My Orders</title>
</head>
<body>
    <h1>My Orders</h1>
    <table>
        <thead>
            <tr>
                <th>Created On</th> 
                <th>Order Price</th>
                <td>User Email</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var order in Model)
            {
                <tr>
                    <td>@User.Identity.Name</td>
                    <td>@order.CreatedOn</td> 
                    <td>@order.OrderPrice.ToString("f2")</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>